    <div class="container" id= "chatShadow">
      <div class="chat-container">
        <header class="chat-header">
          <h1>Gamer Gabble</h1>
          <a href="index.html" class="btn btn-outline-success my-2 my-sm-0">Leave Room</a>
        </header>
        <main class="chat-main">
          <div class="chat-sidebar">
            <h3><i class="fas fa-comments"></i> Room:</h3>
            <h2 id="room-name">JavaScript</h2>
            <h3><i class="fas fa-users"></i> Users</h3>
            <ul id="users">
            </ul>
          </div>
          <div class="chat-messages">
          </div>
        </main>
        <div class="chat-form-container">
          <form id="chat-form">
            <input
              id="msg"
              type="text"
              placeholder="Enter Message"
              required
              autocomplete="off"
            />
            <button class="btn btn-outline-success my-2 my-sm-0" id="sendBtn"><i class="fas fa-paper-plane"></i> Send</button>
          </form>
        </div>
      </div>
    </div>